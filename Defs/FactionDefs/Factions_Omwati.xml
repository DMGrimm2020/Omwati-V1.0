            <li>Caravan_Omwati_Exotic</li>
        </caravanTraderKinds>
        <visitorTraderKinds>
            <li>Visitor_Omwati_Standard</li>
        </visitorTraderKinds>
        <baseTraderKinds>
            <li>Base_Omwati_Standard</li>
        </baseTraderKinds>
        <allowedArrivalTemperatureRange>
            <min>-40</min>
            <max>24</max>
