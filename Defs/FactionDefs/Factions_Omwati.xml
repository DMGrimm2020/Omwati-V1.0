                <li>(0,35)</li>
                <li>(70, 50)</li>
                <li>(700, 100)</li>
                <li>(1300, 150)</li>
                <li>(100000, 10000)</li>
            </points>
        </maxPawnCostPerTotalPointsCurve>
        <pawnGroupMakers>
            <li>
                <!-- Normal fights, standard mix -->
                <kindDef>Combat</kindDef>
                <options>
					<Omwati_Villager>5</Omwati_Villager>
                    <Omwati_Town_Guard>10</Omwati_Town_Guard>
                    <Omwati_Town_Councilman>10</Omwati_Town_Councilman>
                </options>
            </li>
            <li>
                <kindDef>Peaceful</kindDef>
                <options>
                    <Omwati_Villager>20</Omwati_Villager>
                    <Omwati_Town_Guard>10</Omwati_Town_Guard>
                    <Omwati_Town_Councilman>10</Omwati_Town_Councilman>
                </options>
            </li>
            <li>
                <kindDef>Trader</kindDef>
                <traders>
                    <Omwati_Town_Trader>1</Omwati_Town_Trader>
                </traders>
                <carriers>
                    <Muffalo>6</Muffalo>
                    <Dromedary>5</Dromedary>
                    <Alpaca>2</Alpaca>
                    <Elephant>1</Elephant>
                </carriers>
                <guards>
                    <Omwati_Villager>3</Omwati_Villager>
                    <Omwati_Town_Guard>10</Omwati_Town_Guard>
				</guards>
            </li>
            <li>
                <kindDef>Settlement</kindDef>
                <options>
				    <Omwati_Villager>10</Omwati_Villager>
                    <Omwati_Town_Guard>10</Omwati_Town_Guard>
                    <Omwati_Town_Councilman>10</Omwati_Town_Councilman>
                </options>
            </li>
        </pawnGroupMakers>
        <homeIconPath>World/DefaultFactionBase</homeIconPath>
