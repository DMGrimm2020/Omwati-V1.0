		<settlementNameMaker>NamerSettlementOutlander</settlementNameMaker>
		<techLevel>Spacer</techLevel>
	    <backstoryCategories>
            <li>Omwati_Backstory</li>
        </backstoryCategories>
		<geneticVariance>0.8</geneticVariance>
        <goodwillDailyGain>0.25</goodwillDailyGain>
