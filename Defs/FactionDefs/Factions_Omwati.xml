<?xml version="1.0" encoding="utf-8" ?>

<Defs>

    <FactionDef>
	    <defName>Omwatioutlander</defName>
	    <label>Omwati Outpost</label>
        <description>Mysterious Omwati Race</description>
        <pawnSingular>Omwati</pawnSingular>
        <pawnsPlural>Omwatis</pawnsPlural>
        <settlementGenerationWeight>1</settlementGenerationWeight>
        <requiredCountAtGameStart>1</requiredCountAtGameStart>
        <canMakeRandomly>true</canMakeRandomly>
        <canSiege>true</canSiege>
        <canStageAttacks>true</canStageAttacks>
        <expandingIconTexture>World/Expanding/OmwatiFactionBase</expandingIconTexture>
