<?xml version="1.0" encoding="utf-8" ?>

<Defs>
<!-- =Omwati Base = -->

    <ThingDef Name="BaseOmwati" Abstract="True">
        <thingClass>Pawn</thingClass>
        <category>Pawn</category>
        <selectable>true</selectable>
        <tickerType>Normal</tickerType>
        <altitudeLayer>Pawn</altitudeLayer>
        <useHitPoints>false</useHitPoints>
        <hasTooltip>true</hasTooltip>
        <soundImpactDefault>BulletImpact_Flesh</soundImpactDefault>
        <statBases>
            <Mass>70</Mass>
        </statBases>
        <inspectorTabs>
            <li>ITab_Pawn_Health</li>
            <li>ITab_Pawn_Needs</li>
            <li>ITab_Pawn_Character</li>
            <li>ITab_Pawn_Training</li>
            <li>ITab_Pawn_Social</li>
            <li>ITab_Pawn_Guest</li>
            <li>ITab_Pawn_Prisoner</li>
            <li>ITab_Pawn_FormingCaravan</li>
            <li>ITab_Pawn_Gear</li>
            <li>ITab_Pawn_Log</li>
        </inspectorTabs>
        <comps>
            <li>
                <compClass>CompAttachBase</compClass>
            </li>
        </comps>
        <drawGUIOverlay>true</drawGUIOverlay>
    </ThingDef>
    <AlienRace.RaceSettings>
		<defName>Omwati_RaceSettings</defName> 
        <pawnKindSettings>
            <alienslavekinds>
                <li>
                    <kindDefs>
                        <li>Omwati_Slave</li>
                    </kindDefs>
                    <chance>0.60</chance>
                </li>
            </alienslavekinds>
            <alienrefugeekinds>
                <li> 
                    <kindDefs>
	    		        <li>Omwati_Villager</li>
                        <li>Omwati_Town_Guard</li>
                        <li>Omwati_Town_Trader</li>
                        <li>Omwati_Town_Councilman</li>
                    </kindDefs> 
                    <chance>50.0</chance>                        
                </li>
            </alienrefugeekinds>
            <startingColonists>
			    <li>
			        <pawnKindEntries>
	    		        <li>
                            <kindDefs>
		    		            <li>Omwati_Villager</li>
                                <li>Omwati_Town_Guard</li>
                                <li>Omwati_Town_Trader</li>
                                <li>Omwati_Town_Councilman</li>
                            </kindDefs>
                            <chance>10.0</chance>
				        </li>
			        </pawnKindEntries>                       
				    <factionDefs>
					   <li>OmwatiPlayerColony</li> 
	    		    </factionDefs>
                </li>               
    	    </startingColonists>
            <alienwandererkinds>
                <li> 
                    <pawnKindEntries>
                        <li>
                            <kindDefs>
                                <li>Omwati_Villager</li>
                                <li>Omwati_Town_Guard</li>
                                <li>Omwati_Town_Trader</li>
                                <li>Omwati_Town_Councilman</li>
                            </kindDefs> 
                            <chance>50.0</chance>
                        </li>
                    </pawnKindEntries>
                    <factionDefs>
					   <li>OmwatiPlayerColony</li> 
				    </factionDefs>                         
                </li>
            </alienwandererkinds>
        </pawnKindSettings>          
    </AlienRace.RaceSettings>      
    <AlienRace.ThingDef_AlienRace ParentName="BaseOmwati">
        <defName>Alien_Omwati</defName>
        <label>Omwati</label>
        <description>?</description>
        <statBases>
            <MarketValue>1750</MarketValue>
            <MoveSpeed>4.61</MoveSpeed>
            <Flammability>1.0</Flammability>
            <ComfyTemperatureMin>-40</ComfyTemperatureMin>
            <ComfyTemperatureMax>38</ComfyTemperatureMax>
            <LeatherAmount>50</LeatherAmount>
        </statBases>
        <tools>
            <li>
                <label>left fist</label>
                <capacities>
                    <li>Blunt</li>
                </capacities>
                <power>7</power>
                <cooldownTime>1.7</cooldownTime>
                <linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
                <surpriseAttack>
                    <extraMeleeDamages>
                        <li>
                            <def>Stun</def>
                            <amount>14</amount>
                        </li>
                    </extraMeleeDamages>
                </surpriseAttack>
            </li>
            <li>
                <label>right fist</label>
                <capacities>
                    <li>Blunt</li>
                </capacities>
                <power>7</power>
                <cooldownTime>1.7</cooldownTime>
                <linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
                <surpriseAttack>
                    <extraMeleeDamages>
                        <li>
                            <def>Stun</def>
                            <amount>14</amount>
                        </li>
                    </extraMeleeDamages>
                </surpriseAttack>
            </li>
            <li>
                <label>head</label>
                <capacities>
                    <li>Blunt</li>
                </capacities>
                <power>5</power>
                <cooldownTime>1.9</cooldownTime>
                <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
                <chanceFactor>0.2</chanceFactor>
            </li>
        </tools>
        <race>
            <thinkTreeMain>Humanlike</thinkTreeMain>
            <thinkTreeConstant>HumanlikeConstant</thinkTreeConstant>
            <intelligence>Humanlike</intelligence>
            <makesFootprints>true</makesFootprints>
            <lifeExpectancy>85</lifeExpectancy>
	        <leatherDef>Omwati_Leather</leatherDef>
    	    <meatLabel>Omwati meat</meatLabel>
            <nameCategory>HumanStandard</nameCategory>
            <body>Human</body>
            <baseBodySize>1.25</baseBodySize>
            <baseHealthScale>1</baseHealthScale>
            <foodType>OmnivoreHuman</foodType>
            <gestationPeriodDays>45</gestationPeriodDays>
            <litterSizeCurve>
                <points>
                    <li>(0.5, 0)</li>
                    <li>(1, 1)</li>
                    <li>(1.01, 0.02)</li>
                    <li>(3.5, 0)</li>
                </points>
            </litterSizeCurve>
            <lifeStageAges>
                <li>
                    <def>OmwatiBaby</def>
                    <minAge>0</minAge>
                </li>
                <li>
                    <def>OmwatiYoungAdult</def>
                    <minAge>13</minAge>
                </li>
                <li>
                    <def>OmwatiAdult</def>
                    <minAge>18</minAge>
                </li>
                <li>
                    <def>OmwatiMiddleAge</def>
                    <minAge>41</minAge>
                </li>
		        <li>
                    <def>OmwatiOldAge</def>
                    <minAge>66</minAge>
                </li>
		        <li>
                    <def>OmwatiVenerable</def>
                    <minAge>85</minAge>
                </li>
            </lifeStageAges>
            <soundMeleeHitPawn>Pawn_Melee_Punch_HitPawn</soundMeleeHitPawn>
            <soundMeleeHitBuilding>Pawn_Melee_Punch_HitBuilding</soundMeleeHitBuilding>
            <soundMeleeMiss>Pawn_Melee_Punch_Miss</soundMeleeMiss>
            <specialShadowData>
                <volume>(0.3, 0.8, 0.4)</volume>
                <offset>(0,0,-0.3)</offset>
            </specialShadowData>
            <ageGenerationCurve>
                <points>
                    <li>(0,0)</li>
                    <li>(13,100)</li>
                    <li>(18,100)</li>
                    <li>(41,30)</li>
                    <li>(66,18)</li>
                    <li>(85,0)</li>
                </points>
            </ageGenerationCurve>
            <hediffGiverSets>
                <li>OrganicStandard</li>
                <li>Human</li>
            </hediffGiverSets>
        </race>    
        <alienRace>   
            <generalSettings>
                <humanRecipeImport>true</humanRecipeImport>
                <immuneToAge>false</immuneToAge>
                <forcedRaceTraitEntries>
                </forcedRaceTraitEntries>    
                <alienPartGenerator>
				    <alienbodytypes>
					    <li>Female</li>
					    <li>Male</li>
					    <li>Thin</li>
                    </alienbodytypes>
                    <aliencrowntypes>
                        <li>Narrow_Pointy</li>                    
                    </aliencrowntypes>
                    <useGenderedHeads>true</useGenderedHeads>
                    <useGenderedBodies>true</useGenderedBodies>
					<alienhaircolorgen Class="ColorGenerator_Options">
						<options>
                        
							<!--  White Hair Color -->

                            <li>
                                <weight>10</weight>
                                <only>RGBA(255,255,255)</only>
                            </li>
			                <li>
                                <weight>5</weight>
                                <only>RGBA(255,255,255)</only>
                            </li>
			                <li>
                                <weight>2</weight>
                                <only>RGBA(255,255,255)</only>
                            </li>
                        
							<!--  Red Hair Color -->

                            <li>
                                <weight>10</weight>
                                <only>RGBA(204,51,0)</only>
                            </li>
			                <li>
                                <weight>5</weight>
                                <only>RGBA(204,51,0)</only>
                            </li>
			                <li>
                                <weight>2</weight>
                                <only>RGBA(204,51,0)</only>
                            </li>
                        </options>
                    </alienhaircolorgen>   
                    <alienskincolorgen Class="ColorGenerator_Options">
                        <options>
                        
							<!--  Gray/Blue Skin Color -->

                            <li>
                                <weight>10</weight>
                                <only>RGBA(137,159,181)</only>
                            </li>
			                <li>
                                <weight>5</weight>
                                <only>RGBA(137,159,181)</only>
                            </li>
			                <li>
                                <weight>2</weight>
                                <only>RGBA(137,159,181)</only>
                            </li>
                        </options>
                    </alienskincolorgen>
                </alienPartGenerator>
            </generalSettings>
            <graphicPaths>
                <li>
                    <body>Things/Omwati/Bodies/</body>
                    <head>Things/Omwati/Heads/</head>
                    <skeleton/>
                    <skull/>
                </li>
            </graphicPaths>
            <hairSettings>
                <hasHair>true</hasHair>
            </hairSettings>
	        <thoughtSettings>
                <cannotReceiveThoughts>
                </cannotReceiveThoughts>
	        </thoughtSettings>
        </alienRace>
        <recipes>
            <li>InstallArchotechEye</li>
            <li>InstallArchotechArm</li>
            <li>InstallArchotechLeg</li>
            <li>InstallPowerClaw</li>
            <li>InstallBionicEye</li>
            <li>InstallBionicArm</li>
            <li>InstallBionicLeg</li>
            <li>InstallBionicSpine</li>
            <li>InstallBionicHeart</li>
            <li>InstallBionicStomach</li>
            <li>InstallBionicEar</li>
            <li>InstallSimpleProstheticArm</li>
            <li>InstallSimpleProstheticLeg</li>
            <li>InstallSimpleProstheticHeart</li>
            <li>InstallCochlearImplant</li>
            <li>InstallPegLeg</li>
            <li>InstallWoodenFoot</li>
            <li>InstallDenture</li>
            <li>InstallJoywire</li>
            <li>InstallPainstopper</li>
            <li>InstallNaturalHeart</li>
            <li>InstallNaturalLung</li>
            <li>InstallNaturalKidney</li>
            <li>InstallNaturalLiver</li>
            <li>ExciseCarcinoma</li>
            <li>AdministerMechSerumHealer</li>
            <li>RemoveBodyPart</li>
            <li>Euthanize</li>
            <li>Anesthetize</li>
        </recipes>    
    </AlienRace.ThingDef_AlienRace>

</Defs>
